(define (problem rover_problem)
    (:domain rover)
    (:objects r - robot
              p1010 p1324 p1811 p2519 p3418 - coordinate
              slow fast - speed
    )
    (:init (= (distance p1010 p1324) 14.3)
           (= (distance p1324 p1010) 14.3)
           (= (distance p1010 p1811) 8.1)
           (= (distance p1811 p1010) 8.1)
           (= (distance p1010 p2519) 17.5)
           (= (distance p2519 p1010) 17.5)
           (= (distance p1010 p3418) 25.3)
           (= (distance p3418 p1010) 25.3)
           (= (distance p1324 p1811) 13.9)
           (= (distance p1811 p1324) 13.9)
           (= (distance p1324 p2519) 13)
           (= (distance p2519 p1324) 13)
           (= (distance p1324 p3418) 21.8)
           (= (distance p3418 p1324) 21.8)
           (= (distance p1811 p2519) 10.6)
           (= (distance p2519 p1811) 10.6)
           (= (distance p1811 p3418) 17.5)
           (= (distance p3418 p1811) 17.5)
           (= (distance p2519 p3418) 9.1)
           (= (distance p3418 p2519) 9.1)

           (at r p1811)

           (iddle r)

           (= (robot-speed slow) 1)
           (= (robot-speed fast) 2)

           (= (battery-capacity r) 100)
           (= (battery-amount r) 80)
           (= (low-battery r) 15)
    )
    (:goal (and (photo p1324)
                (drilled p1324)
                (drilled p1010)
                (communicated r p2519)
                (analyzed p3418)
                (analyzed p1811)
            )
    )
    (:constraints (and (preference DANGEROUS (always (not (at r p2519))))
                       (preference PICTURE (sometime (photo p3418))))
    )
)